<div class="rule-action-item" id="<%= method_id %>" touch="true">
	<span class="delete-action" class="clear" style="display:<%= (edit_mode === false) ? 'none' : 'block' %>" touch="true">
		<a href="<%= delete_url %>"class="nav-circle-delete icon-delete">&#xe605 </a>
	</span>
	<span id="error" class="error" style="<%= (!is_error) ? 'display:none' : '' %>">&#xe634</span>
	<span id="rule-action-desc" class="itm-chop"><%= desc %></span>
</div>
