/*global define*/

define([
    'underscore',
    'libs/backbone/models/kurobox-model',
], function (_, KuroboxModel) {
    'use strict';

    var RemoveCookie = KuroboxModel.extend({

        fetch: function(options) {
            this.url = 'remove_cookie';
            this.dev_opt = {
                bypassSession: true,
                method: 'GET',
                param: {}
            };

            KuroboxModel.prototype.fetch.call(this, options);            
        },

        parse: function(data) {
        }
    });

    var CheckCookie = KuroboxModel.extend({

        fetch: function(options) {
            this.url = 'check_cookie';
            this.dev_opt = {
                bypassSession: true,
                method: 'GET',
                param: {}
            };

            KuroboxModel.prototype.fetch.call(this, options);            
        },

        parse: function(data) {
        }
    });

    var CookieBundle = {
        Remove:RemoveCookie,
        Check:CheckCookie
    }
    return CookieBundle;
});
